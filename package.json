{
  "name": "gatsby-blog",
  "description": "Gatsby medium blog",
  "version": "2.0.0",
  "author": "Theo Dammaretz <theo@dammaretz.fr>",
  "dependencies": {
    "babel-plugin-styled-components": "1.10.6",
    "gatsby": "2.18.20",
    "gatsby-image": "2.4.9",
    "gatsby-plugin-alias-imports": "1.0.5",
    "gatsby-plugin-csp": "1.1.3",
    "gatsby-plugin-offline": "3.0.30",
    "gatsby-plugin-react-axe": "0.3.0",
    "gatsby-plugin-react-helmet": "3.1.20",
    "gatsby-plugin-react-svg": "3.0.0",
    "gatsby-plugin-sharp": "2.6.14",
    "gatsby-plugin-styled-components": "3.1.16",
    "gatsby-plugin-typescript": "2.1.25",
    "gatsby-plugin-webpack-bundle-analyzer": "1.0.5",
    "gatsby-source-filesystem": "2.3.14",
    "gatsby-transformer-sharp": "2.5.7",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0",
    "styled-components": "4.4.1",
    "typeface-roboto-mono": "0.0.75"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "CC-BY-4.0",
  "scripts": {
    "prebuild": "yarn run test",
    "serve": "serve public",
    "build": "gatsby build",
    "start": "gatsby develop",
    "format": "prettier --write 'src/**/*.tsx'",
    "test": "yarn run test:type && yarn run test:style && yarn run test:e2e",
    "test:ci": "yarn run test:style && yarn run test:e2e",
    "test:type": "tsc --noEmit",
    "test:style": "tslint --project .",
    "test:e2e": "start-server-and-test start http://localhost:8000 cy:run",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "predeploy": "CI=1; yarn add cypress; yarn run test:ci",
    "deploy": "yarn run gatsby build"
  },
  "devDependencies": {
    "@types/react-helmet": "6.0.0",
    "@types/styled-components": "5.1.0",
    "cypress": "4.9.0",
    "prettier": "2.0.5",
    "serve": "11.3.2",
    "start-server-and-test": "1.11.0",
    "tslint": "6.1.2",
    "tslint-config-prettier": "1.18.0",
    "tslint-react": "5.0.0",
    "typescript": "3.9.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Blightwidow/gatsby-blog"
  }
}
