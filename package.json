{
  "name": "gatsby-blog",
  "description": "Gatsby medium blog",
  "version": "1.0.0",
  "author": "Theo Dammaretz <theo@dammaretz.fr>",
  "dependencies": {
    "babel-plugin-styled-components": "^1.7.1",
    "dayjs": "^1.7.7",
    "gatsby": "^2.0.11",
    "gatsby-plugin-google-analytics": "^2.0.6",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-styled-components": "^3.0.0",
    "gatsby-plugin-typescript": "^2.0.0",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.3",
    "gatsby-plugin-workbox": "^0.1.7",
    "gatsby-source-medium": "^2.0.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-helmet": "^5.2.0",
    "react-particles-js": "^2.3.0",
    "styled-components": "^3.4.4",
    "typeface-open-sans": "^0.0.54",
    "typeface-rubik": "^0.0.54"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "prebuild": "yarn run test",
    "build": "gatsby build",
    "start": "gatsby develop",
    "serve": "serve public",
    "format": "prettier --write 'src/**/*.tsx'",
    "test": "yarn run test:type && yarn run test:style && yarn run test:e2e",
    "test:ci": "yarn run test:style && yarn run test:e2e",
    "test:type": "tsc --noEmit",
    "test:style": "tslint --project .",
    "test:e2e": "start-server-and-test start http://localhost:8000 cy:run",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "predeploy": "CI=1; yarn add cypress; yarn run test:ci",
    "deploy": "gatsby build"
  },
  "devDependencies": {
    "@types/react-helmet": "^5.0.7",
    "cypress": "^3.1.0",
    "prettier": "^1.14.3",
    "serve": "^10.0.2",
    "start-server-and-test": "^1.7.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Blightwidow/gatsby-blog"
  }
}
