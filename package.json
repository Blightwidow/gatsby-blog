{
  "name": "gatsby-blog",
  "description": "Gatsby medium blog",
  "version": "1.0.0",
  "author": "Theo Dammaretz <theo@dammaretz.fr>",
  "dependencies": {
    "@ctrl/tinycolor": "^2.4.0",
    "@types/react-intl": "^2.3.17",
    "babel-plugin-styled-components": "^1.10.0",
    "gatsby": "^2.3.3",
    "gatsby-plugin-google-analytics": "^2.0.18",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-react-helmet": "^3.0.11",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-plugin-typescript": "^2.0.11",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.4",
    "gatsby-source-medium": "^2.0.6",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-helmet": "^5.2.0",
    "react-intl": "^2.8.0",
    "react-particles-js": "^2.5.1",
    "styled-components": "^4.2.0",
    "typeface-open-sans": "^0.0.54",
    "typeface-rubik": "^0.0.72"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "prebuild": "yarn run test",
    "build": "gatsby build",
    "start": "gatsby develop",
    "serve": "serve public",
    "format": "prettier --write 'src/**/*.tsx'",
    "test": "yarn run test:type && yarn run test:style && yarn run test:e2e",
    "test:ci": "yarn run test:style && yarn run test:e2e",
    "test:type": "tsc --noEmit",
    "test:style": "tslint --project .",
    "test:e2e": "start-server-and-test start http://localhost:8000 cy:run",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "predeploy": "CI=1; yarn add cypress; yarn run test:ci",
    "deploy": "gatsby build"
  },
  "devDependencies": {
    "@types/react-helmet": "^5.0.8",
    "@types/styled-components": "^4.1.12",
    "@types/tinycolor2": "^1.4.1",
    "cypress": "^3.2.0",
    "prettier": "^1.16.4",
    "serve": "^10.1.2",
    "start-server-and-test": "^1.7.12",
    "tslint": "^5.14.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.3.4000"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Blightwidow/gatsby-blog"
  }
