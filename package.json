{
  "name": "gatsby-blog",
  "description": "Gatsby medium blog",
  "version": "2.0.0",
  "author": "Theo Dammaretz <theo@dammaretz.fr>",
  "dependencies": {
    "babel-plugin-styled-components": "^1.10.0",
    "gatsby": "^2.4.2",
    "gatsby-plugin-google-analytics": "^2.0.19",
    "gatsby-plugin-offline": "^2.1.0",
    "gatsby-plugin-react-axe": "^0.2.2",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-plugin-typescript": "^2.0.13",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "styled-components": "^4.2.0",
    "typeface-roboto": "^0.0.54",
    "typeface-roboto-mono": "^0.0.54"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "prebuild": "yarn run test",
    "build": "gatsby build",
    "start": "gatsby develop",
    "serve": "serve public",
    "format": "prettier --write 'src/**/*.tsx'",
    "test": "yarn run test:type && yarn run test:style && yarn run test:e2e",
    "test:ci": "yarn run test:style && yarn run test:e2e",
    "test:type": "tsc --noEmit",
    "test:style": "tslint --project .",
    "test:e2e": "start-server-and-test start http://localhost:8000 cy:run",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "predeploy": "CI=1; yarn add cypress; yarn run test:ci",
    "deploy": "gatsby build"
  },
  "devDependencies": {
    "@types/react-helmet": "^5.0.8",
    "@types/styled-components": "^4.1.14",
    "cypress": "^3.2.0",
    "prettier": "^1.17.0",
    "serve": "^11.0.0",
    "start-server-and-test": "^1.9.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Blightwidow/gatsby-blog"
  }
}
